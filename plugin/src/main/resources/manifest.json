{
  // MT插件系统SDK版本
  // 高版本插件无法安装在低版本插件系统中
  // 目前均填2
  pluginSdkVersion: 2,
  // 插件ID，插件的唯一标识，类似于packageName
  // 只能由字母、数字、下划线和点组成
  pluginID: "bin.mt.plugin.translation",
  // 插件版本号，低版本插件无法覆盖安装高版本插件
  versionCode: 13,
  // 插件版本名称
  versionName: "v1.8",
  // 插件名称
  name: "{plugin_name}",
  // 插件描述
  description: "{plugin_description}",
  // 对外接口，接口类型会自动判断
  // 目前仅有翻译接口
  interfaces: [
    "bin.mt.plugin.GoogleWebTranslationEngine",
    "bin.mt.plugin.BaiduWebTranslationEngine",
    "bin.mt.plugin.BaiduTranslationEngine",
    "bin.mt.plugin.YandexTranslationEngine"
  ],

  // 主设置界面，可选
  mainPreference: "bin.mt.plugin.Preference"
}